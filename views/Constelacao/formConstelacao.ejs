<form action="<%= formAction %>" method="post">
    <div class="form-group">
      <label for="nome">Nome</label>
      <input type="text" name="nome" id="nome" class="form-control" value="<%= estrela.nome %>" />
    </div>
    <div class="form-group">
      <label for="descricao">Descrição</label>
      <input type="text" name="descricao" id="descricao" class="form-control" value="<%= estrela.descricao %>" />
    </div>
    <div class="form-group">
      <label for="magnitude">Magnitude Aparente</label>
      <input type="text" name="magnitude" id="magnitude" class="form-control" value="<%= estrela.magnitude %>" />
    </div>
    <div class="form-group">
      <label for="tamanho">Tamanho</label>
      <input type="text" name="tamanho" id="tamanho" class="form-control" value="<%= estrela.tamanho %>" />
    </div>
    <div class="form-group">
      <label for="classificacao">Classificação Estelar</label>
      <input type="text" name="classificao" id="classificacao" class="form-control" value="<%= estrela.classificacao %>" />
    </div>
    <div class="form-group">
      <label for="massa">Massa</label>
      <input type="text" name="massa" id="massa" class="form-control" value="<%= estrela.massa %>" />
    </div>
    <div class="form-group">
      <label for="distancia">Distância</label>
      <input type="text" name="distancia" id="distancia" class="form-control" value="<%= estrela.distancia %>" />
    </div>
    <div class="form-group">
      <label for="constelacao">Constelação</label>
      <input type="text" name="constelacao" id="constelacao" class="form-control" value="<%= estrela.constelacao %>" />
    </div>
    <button class="btn btn-primary" type="submit">Salvar Prova</button>
  </form>